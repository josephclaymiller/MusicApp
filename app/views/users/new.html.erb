<h1>Sign Up</h1>

<form action="<%= users_url %>" method="POST">
	<input
	   name="authenticity_token"
	   type="hidden"
	   value="<%= form_authenticity_token %>">
	   
   <label 
	   for="user_email">
	   Email
   </label>
   <input type="text" 
	   name="user[email]" 
	   id="user_email">
   <br />
   
   <label 
	   for="user_password">
	   Password
   </label>
   <input type="password" 
	   name="user[password]" 
	   id="user_password">
   <br />
   
   <input type="submit" value="Sign up">
</form>